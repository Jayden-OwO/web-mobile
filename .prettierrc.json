{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 4,
  "useTabs": false,
  "printWidth": 180,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "requirePragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "rangeStart": 0
}
